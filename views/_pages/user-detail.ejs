<h2>User Details</h2>
<div class="users">
	<form>
		<div>
			<label for="username">Username:</label>
			<input type="text" id="username" name="user_name" value="<%= data.user.user_name %>" />
		</div>
		<div>
			<label for="first">First:</label>
			<input type="text" id="first" name="first_name" value="<%= data.user.first_name %>" />
		</div>
		<div>
			<label for="last">Last:</label>
			<input type="text" id="last" name="last_name" value="<%= data.user.last_name %>" />
		</div>
		<div>
			<label for="email">Email:</label>
			<input type="text" id="email" name="email" value="<%= data.user.email %>" />
		</div>
		<div>
			<label for="phone">Phone:</label>
			<input type="text" id="phone" name="phone" value="<%= data.user.phone %>" />
		</div>
	</form>
</div>
